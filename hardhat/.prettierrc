{
  "printWidth": 120,
  "singleQuote": false,
  "useTabs": false,
  "tabWidth": 2,
  "trailingComma": "none",
  "semi": true,
  "proseWrap": "always",
  "overrides": [
    {
      "files": "*.sol",
      "options": {
        "tabWidth": 4,
        "bracketSpacing": false,
        "explicitTypes": "always"
      }
    }
  ]
}
