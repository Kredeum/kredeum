{
  "name": "kredeum",
  "version": "1.6.2",
  "main": "index.js",
  "scripts": {
    "clean": "pnpm -r clean && rm -rf node_modules .turbo web/dapp",
    "all": "pnpm clean && pnpm install && pnpm build",
    "build": "turbo build",
    "dev:dapp": "turbo dev -F sveltekit",
    "build:dapp": "turbo build -F sveltekit",
    "build:plugin": "turbo build -F wordpress",
    "deploy:dapp": "pnpm install && pnpm build:dapp",
    "deploy:plugin": "pnpm install && pnpm build:plugin"
  },
  "keywords": [
    "Kredeum",
    "NFT",
    "Factory"
  ],
  "author": "Kredeum",
  "license": "MIT",
  "packageManager": "pnpm@8.15.7",
  "engines": {
    "node": "^20",
    "pnpm": "^9"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^7.13.1",
    "@typescript-eslint/parser": "^7.13.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "prettier": "^3.3.2",
    "ts-node": "^10.9.2",
    "turbo": "^1.13.4"
  },
  "type": "module"
}