{
  "name": "@kredeum/kredeum-nfts",
  "scripts": {
    "all": "npm run clean && npm run instal && npm run build && npm run check && npm run tests",
    "clean": "npm run smart-clean && npm run dapp-clean && npm run wp-clean && npm run sub-clean && rm -rf node_modules package-lock.json packages-pnpm-lock.yaml",
    "instal": "pnpm install && npm run wp-install && cypress install",
    "build": "npm run smart-build && npm run dapp-build && npm run wp-build",
    "check": "npm run smart-check && npm run dapp-check && npm run wp-check",
    "tests": "npm run smart-tests && npm run dapp-tests && npm run wp-tests",
    "github": "npm run smart-build && npm run smart-tests",
    "smart-clean": "rm -rf solidity/artifacts",
    "smart-build": "cd solidity && hardhat compile && hardhat docgen",
    "smart-prettier": "prettier solidity/contracts/**/*.sol lib/*.ts solidity/**/*.ts --write --loglevel error",
    "smart-solhint": "solhint solidity/contracts/**/*.sol --fix",
    "smart-eslint": "eslint hardhat.config.ts lib/*.ts solidity/**/*.ts --ignore-pattern types --fix --quiet",
    "smart-check": "npm run smart-prettier && npm run smart-solhint && npm run smart-eslint",
    "smart-tests": "cd solidity && hardhat test",
    "smart-flatten": "cd solidity && hardhat flatten solidity/contracts/OpenNFTs.sol > solidity/flatten/OpenNFTs.sol",
    "smart-deploy": "cd solidity && hardhat run scripts/deploy.mjs --network ${NETWORK:-mumbai}",
    "smart-deploy-matic": "NETWORK=matic npm run smart-deploy",
    "smart-list": "hardhat run scripts/list.mjs",
    "dapp-clean": "rm -rf web/app",
    "dapp-build": "npm run dapp-clean && gulp && npm run dapp-prod",
    "dapp-prod": "GIT_SHORT=$(git rev-parse --short HEAD) GIT_BRANCH=$(git rev-parse --abbrev-ref HEAD) rollup -c --configPlugin typescript",
    "dapp-dev": "GIT_SHORT=$(git rev-parse --short HEAD) GIT_BRANCH=$(git rev-parse --abbrev-ref HEAD) rollup -c -w --configPlugin typescript",
    "dapp-prettier": "prettier svelte/*.svelte svelte/*.ts lib/*.ts --write --loglevel error",
    "dapp-eslint": "eslint svelte/*.svelte svelte/*.ts lib/*.ts --fix --quiet",
    "dapp-svelte-check": "svelte-check",
    "dapp-check": "npm run dapp-prettier && npm run dapp-eslint",
    "dapp-tests": "",
    "dapp--tests": "cd web && cypress run",
    "dapp-start": "sirv web",
    "dapp-deploy-beta": "sh ./sh/dapp-deploy-beta",
    "wp-clean": "npm run wp-clean-kredeum && rm -rf  wordpress/vendor",
    "wp-clean-kredeum": "rm -rf wordpress/kredeum-nfts/lib wordpress/kredeum-nfts/vendor wordpress/kredeum-nfts/readme.txt wordpress/kredeum-nfts/kredeum-nfts.php",
    "wp-install": "rm -rf wordpress/vendor && composer install --prefer-dist --no-progress",
    "wp-dev": "npm run dapp-dev",
    "wp-build-txt": "hbs-cli -D config/config.json wordpress/readme.txt_handlebars -o wordpress/kredeum-nfts -e txt",
    "wp-build-php": "hbs-cli -D config/config.json wordpress/kredeum-nfts.php_handelbars -o wordpress/kredeum-nfts -e php",
    "wp-build-vendor": "cd wordpress/kredeum-nfts && composer install --prefer-dist --no-progress",
    "wp-build-dapp": "gulp && rollup -c --configPlugin typescript",
    "wp-build": "npm run wp-clean-kredeum && npm run wp-build-dapp && cp web/app/assets/kredeum-nfts.css wordpress/kredeum-nfts/lib/css && npm run wp-build-txt && npm run wp-build-php && npm run wp-build-vendor",
    "wp-fix": "wordpress/vendor/bin/phpcbf wordpress/kredeum-nfts --ignore=vendor --extensions=php --standard=WordPress",
    "wp-format": "wordpress/vendor/bin/phpcs wordpress/kredeum-nfts -n --ignore=vendor --extensions=php --standard=WordPress",
    "wp-check": "npm run wp-fix && npm run wp-format",
    "wp-tests": "",
    "wp--tests": "cd wordpress && cypress run",
    "sub-clean": "rm -rf thegraph/build thegraph/generated thegraph/subgraph.yaml",
    "sub-yaml": "jq '.[]|select(.chainName==\"${NETWORK:-mumbai}\")' config/networks.json | hbs-cli -i thegraph/subgraph.yaml_handlebar -e yaml -o thegraph",
    "sub-codegen": "cd thegraph && graph codegen",
    "sub-graph-build": "cd thegraph && graph build",
    "sub-build": "echo sub-build empty",
    "sub--build": "npm run sub-yaml && npm run sub-codegen && npm run sub-graph-build",
    "sub-check": "prettier --no-error-on-unmatched-pattern thegraph/**/*ts  thegraph/**/*.yaml thegraph/**/*.graphql --write",
    "sub-tests": "mocha --experimental-json-modules thegraph/tests/*.mjs",
    "sub-predeploy": "npm run sub-clean && npm run sub-yaml && npm run sub-codegen && npm run sub-build && npm run sub-check",
    "sub-predeploy-matic": "NETWORK=matic npm run sub-predeploy",
    "sub-deploy": "npm run sub-predeploy && cd thegraph && graph deploy zapaz/eip721-${NETWORK:-mumbai} --ipfs https://api.thegraph.com/ipfs/ --node https://api.thegraph.com/deploy/ --access-token $THEGRAPH_KEY",
    "sub-deploy-matic": "NETWORK=matic npm run sub-deploy",
    "start": "npm run dapp-start"
  },
