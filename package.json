{
  "name": "kredeum",
  "version": "1.4.0",
  "main": "index.js",
  "scripts": {
    "clean": "pnpm -r clean && rm -rf node_modules pnpm-lock.yaml .turbo web/dapp",
    "all": "pnpm clean && pnpm install && pnpm build",
    "build": "turbo build",
    "dev:dapp": "turbo dev -F sveltekit",
    "build:dapp": "turbo build -F sveltekit",
    "build:plugin": "turbo build -F wordpress",
    "deploy:dapp": "pnpm install && pnpm build:dapp",
    "deploy:plugin": "pnpm install && pnpm build:plugin"
  },
  "keywords": [
    "Kredeum",
    "NFT",
    "Factory"
  ],
  "author": "kredeum2",
  "license": "MIT",
  "packageManager": "pnpm@8.15.3",
  "engines": {
    "node": "^20",
    "pnpm": "^8"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^7.6.0",
    "@typescript-eslint/parser": "^7.6.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "prettier": "^3.2.5",
    "ts-node": "^10.9.2",
    "turbo": "^1.13.2"
  }
}